# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Required: Google Gemini API Key
GOOGLE_API_KEY=your_google_api_key_here

# Optional: Backend URL for Streamlit UI (only needed if backend is deployed separately)
# Leave empty for local development (defaults to http://localhost:8000)
BACKEND_URL=

# Optional: Port for FastAPI (cloud platforms usually set this automatically)
PORT=8000
